@article {Dobson2021,
article_type = {journal},
title = {The structure of photosystem I from a high-light-tolerant cyanobacteria},
author = {Dobson, Zachary and Ahad, Safa and Vanlandingham, Jackson and Toporik, Hila and Vaughn, Natalie and Vaughn, Michael and Williams, Dewight and Reppert, Michael and Fromme, Petra and Mazor, Yuval},
editor = {Kramer, David M and Boudker, Olga and Davis, Geoffry A},
volume = 10,
year = 2021,
month = {aug},
pub_date = {2021-08-26},
pages = {e67518},
citation = {eLife 2021;10:e67518},
doi = {10.7554/eLife.67518},
url = {https://doi.org/10.7554/eLife.67518},
misc={spectroscopy,photosynthesis},
abstract = {Photosynthetic organisms have adapted to survive a myriad of extreme environments from the earth’s deserts to its poles, yet the proteins that carry out the light reactions of photosynthesis are highly conserved from the cyanobacteria to modern day crops. To investigate adaptations of the photosynthetic machinery in cyanobacteria to excessive light stress, we isolated a new strain of cyanobacteria, \textit{Cyanobacterium aponinum} 0216, from the extreme light environment of the Sonoran Desert. Here we report the biochemical characterization and the 2.7 Å resolution structure of trimeric photosystem I from this high-light-tolerant cyanobacterium. The structure shows a new conformation of the PsaL C-terminus that supports trimer formation of cyanobacterial photosystem I. The spectroscopic analysis of this photosystem I revealed a decrease in far-red absorption, which is attributed to a decrease in the number of long- wavelength chlorophylls. Using these findings, we constructed two chimeric PSIs in \textit{Synechocystis} sp. PCC 6803 demonstrating how unique structural features in photosynthetic complexes can change spectroscopic properties, allowing organisms to thrive under different environmental stresses.},
keywords = {cyanobacterium aponinum, cyanobacteria, extermophile, photosynthesis, light reactions, photosystems},
journal = {eLife},
issn = {2050-084X},
publisher = {eLife Sciences Publications, Ltd},
}



